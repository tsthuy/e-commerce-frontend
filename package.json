  "packageManager": "npm@10.2.3",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules dist out .env",
    "setup": "npm ci",
    "renew": "npm run clean && npm run setup",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint '**/*.{css,pcss,scss}' && echo 'lint success'",
    "commit": "cz",
    "prepare": "husky install",
    "set-env:local-dev": "yenv print environment.yml local-development dotenv -f -r > .env",
    "dev": "npm run set-env:local-dev && env-cmd -f .env vite",
    "set-env:dev": "yenv print environment.yml development dotenv -f -r > .env",
    "build:dev": "npm run set-env:dev && env-cmd -f .env tsc -b && vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "generate": "plop",
    "treemap": "vite-bundle-visualizer --open --template=treemap --output='./dist/treemap.html'"
  },
